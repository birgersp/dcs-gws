{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"name": "PowerShell: Launch Current File",
			"type": "PowerShell",
			"request": "launch",
			"script": "${file}",
			"cwd": "${workspaceFolder}"
		}
	]
}
